[Unit]
Description=HULK Service
Requires=lola.service hula.service
After=lola.service hula.service

[Service]
ExecStart=/usr/bin/launchHULK
TimeoutStopSec=10
LimitRTPRIO=36
StandardOutput=journal
Type=simple
User=nao
Group=nao
LimitCORE=infinity
WorkingDirectory=/home/nao/hulk

[Install]
WantedBy=default.target

